#### Scripted 2brid
symbol:
	image: /magic-modules.mse-include/symbol-fonts/large/hybrid/_W.png
	code: ([XYZ0-9])/W
	regex: yes
	draw text: 1
	text margin left:   -.25
	text margin right:  .2
	text margin top:    -.14
	text margin bottom: .4
	text font:
		name: MPlantin
		size: .6
		color: rgb(0,0,0)
		max stretch: 0.5
	image font size: 145
